#!/bin/bash
rm -f output/pbutter.log
rm -f output/pbutter.out
rm -f model/pbutterMainSimple.oxo #model/pbutterSimple.oxo model/Inventory.oxo

#oxl -c model/pbutterMainSimple.ox model/pbutterSimple.h model/Inventory.h
oxl -DVHIGH+QUAD+COMPLEX -c model/pbutterMainSimple.ox model/pbutterSimple.h model/Inventory.h
mv pbutterMainSimple.oxo model/.

oxl model/pbutterMainSimple.oxo
