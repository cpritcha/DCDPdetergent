#!/bin/bash

sawDirectory="/home/cpritcha/paper/src"
personalDirectory="/Users/calvinpritchard/Documents"

version=${2:-1}
datafile="data/$1.dta"
logfile="$1$version.log"
resultsfile="$1$version.out"

rm -f ${logfile}
rm -f model/main.oxo
oxl -c model/pbutterMain.ox model/Inventory.h model/pbutter.h
mv pbutterMain.oxo model/.

cmd="sqsub -r 6.0h -o ${logfile} -q serial --memperproc=3.5G oxl model/pbutterMain.oxo ${datafile} ${logfile} ${resultsfile}"
echo "Starting Detergent Estimation"
echo $cmd

eval $cmd
